%div#user_info
  %text= t(:loggedinas) + @user.name
  %br
  = link_to t(:editprofile), edit_dashboard_user_path(@user.id)
  = link_to t(:signout), sessions_logout_path, :id => 'sign_out'

%h1#header= @title

%div#link_column
  - @links.keys.each do |name|
    = link_to name, @links[name], :class => :dashboard_button
  = link_to t(:dashboardhome), '/dashboard', :class => :dashboard_button

%h4#flash= flash[:notice]
    
